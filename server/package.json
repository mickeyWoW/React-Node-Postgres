      "db": "rake-db",
      "db:seeds": "cross-env NODE_PATH=src NODE_ENV=test ts-node src/tests/utils/seeds.ts",
      "lint": "eslint . --ext 'js,ts'",
      "lint:fix": "eslint . --ext 'js,ts' --fix",
      "test": "cross-env NODE_PATH=src jest",
      "pre-commit": "lint-staged"
   },
   "lint-staged": {
      "**/*.ts": "eslint --fix"
   },
